# MongoDB Configuration
MONGODB_URL=mongodb+srv://username:password@cluster.mongodb.net/?appName=YourApp
DATABASE_NAME=your_database_name

# JWT Configuration
SECRET_KEY=your-secret-key-here-generate-a-secure-random-string
